<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="126.0" prefWidth="302.0" spacing="10.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.humanitarian.logistics.userInterface.SearchingController">
    
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <Label fx:id="statusLabel" text="Searching...">
        <font>
            <Font size="14.0" />
        </font>
    </Label>

    <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0" />
   <Button mnemonicParsing="false" onAction="#cancelSearch" text="Cancel Search" />

</VBox>

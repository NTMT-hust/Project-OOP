<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane stylesheets="@InitializeCollector.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.humanitarian.logistics.userinterface.collectdata.intializeCollector.InitializeController">
   <children>
      <VBox fx:id="rootPane" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="500.0">
         <children>
            <Label fx:id="statusLabel" styleClass="title-label" text="Label">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <ProgressBar fx:id="progressBar" prefHeight="20.0" prefWidth="200.0" progress="0.0" styleClass="glass-progress-bar" />
         </children>
      </VBox>
   </children>
</StackPane>

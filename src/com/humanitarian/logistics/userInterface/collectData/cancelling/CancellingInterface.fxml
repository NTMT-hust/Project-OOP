<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<StackPane xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.humanitarian.logistics.userInterface.collectData.cancelling.CancellingController">
   <children>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
          
          <padding>
              <Insets bottom="20" left="20" right="20" top="20" />
          </padding>
      
          <Label text="Cancelling...">
              <font>
                  <Font size="14.0" />
              </font>
          </Label>
      
          <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0" />
      
      </VBox>
   </children>
</StackPane>

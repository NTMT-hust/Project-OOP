<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane stylesheets="@SearchingInterface.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.humanitarian.logistics.userInterface.collectData.searching.SearchingController">
   <children>
      <VBox fx:id="scenePane" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="400.0">
         <children>
            <Label styleClass="title-label" text="Searching...">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <ProgressBar fx:id="progressBar" prefHeight="20.0" prefWidth="250.0" progress="0.0" styleClass="glass-progress-bar" />
            <Label fx:id="statusLabel" text="Label" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</StackPane>
